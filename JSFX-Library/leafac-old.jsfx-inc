  gfx_x = P0.x;
  gfx_y = P0.y;
  t.increment = 1 / steps;
  t = 0; loop(steps, t += t.increment;
    gfx_lineto(_.bezier.cubic(P0.x, P1.x, P2.x, P3.x, t), _.bezier.cubic(P0.y, P1.y, P2.y, P3.y, t), _.true);
  );
);

