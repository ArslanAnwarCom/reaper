// BÉZIER CURVES
// https://en.wikipedia.org/wiki/Bézier_curve
// https://ciechanow.ski/drawing-bezier-curves/

_.bezier.steps_multiplier = 0.13;

function _.bezier.linear(P0, P1, t) (
  P0 + t * (P1 - P0);
